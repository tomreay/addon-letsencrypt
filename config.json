{
  "name": "Let's Encrypt Route 53",
  "version": "3.0",
  "slug": "letsencrypt_route53",
  "description": "Manage certificate from Let's Encrypt using Route 53",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "boot": "manual",
  "map": ["ssl:rw"],
  "options": {
    "email": null,
    "domains": [null],
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "accessKeyId": null,
    "secretAccessKey": null,
    "region": "eu-west-1"
  },
  "schema": {
    "email": "email",
    "domains": ["str"],
    "certfile": "str",
    "keyfile": "str",
    "accessKeyId": "str",
    "secretAccessKey": "str",
    "region": "str"
  }
}
